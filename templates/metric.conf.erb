# Managed by puppet with metric.conf.erb file.
<%= metricid %>
	MetricName	<%= @metricname %>
	MetricClass	<%= @metricclass %>
	Timing		<%= @my_timing %>	<%= @my_offset %>
<% if @my_params and @my_params.length > 0 -%>
	Parameters
<% @my_params.sort.each do |key,val| -%>
		<%= key %>	<%= val %>
<% end -%>
<% end -%>
<% if @my_smoothing and @my_smoothing.length  > 0 -%>
	Smoothing
<% @my_smoothing.sort.each do |key,val| -%>
		<%= key %>	<%= val %>
<% end -%>
<% end -%>

# End of template for metric <%= title %>

